<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Find_result"
    android:id="@+id/find_result"
    android:background="#F9F1D9">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="5dp"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-16dp">

        <TextView
            android:id="@+id/txt_found_id_pw"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="38dp"
            android:layout_marginBottom="9dp"
            android:fontFamily="@font/neo"
            android:text=""
            android:textColor="#B35A0E"
            android:textSize="35dp"
            tools:ignore="MissingConstraints" />

        <TextView
            android:layout_width="409dp"
            android:layout_height="7dp"
            android:layout_marginStart="1dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="22dp"
            android:background="#B35A0E"
            tools:ignore="MissingConstraints" />


        <EditText
            android:id="@+id/found_content"
            android:layout_width="300dp"
            android:layout_height="70dp"
            android:layout_marginTop="50dp"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/framelayout"
            android:ems="10"
            android:inputType="text" />



        <Button
            android:id="@+id/btn_backToLogin"
            android:layout_width="226dp"
            android:layout_height="48dp"
            android:background="@drawable/txt_input"
            android:backgroundTint="#FAE7B0"
            android:buttonTint="#B35A0E"
            android:fontFamily="@font/superbold"
            android:gravity="center"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="38dp"
            android:text="로그인 화면으로 돌아가기"
            android:textColor="#B35A0E" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>